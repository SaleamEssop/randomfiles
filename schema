  `merchant_id` varchar(255) DEFAULT NULL,
  `merchant_key` varchar(255) DEFAULT NULL,
  `active_for_whats_app` tinyint(4) NOT NULL DEFAULT 2,
  `delivery_open_time` time DEFAULT NULL,
  `delivery_close_time` time DEFAULT NULL,
  `store_open_for_sunday` varchar(255) DEFAULT NULL,
  `store_open_for_monday` varchar(255) DEFAULT NULL,
  `store_open_for_tuesday` varchar(255) DEFAULT NULL,
  `store_open_for_wednesday` varchar(255) DEFAULT NULL,
  `store_open_for_thursday` varchar(255) DEFAULT NULL,
  `store_open_for_friday` varchar(255) DEFAULT NULL,
  `store_open_for_saturday` varchar(255) DEFAULT NULL,
